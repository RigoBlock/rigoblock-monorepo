{
  "name": "@rigoblock/pool-api",
  "description": "A JS wrapper around RigoBlock Smart Contracts to simplify their consumption.",
  "version": "0.1.0",
  "engines": {
    "node": ">=6.4"
  },
  "scripts": {
    "build": "rm -rf dist && webpack --config webpack.config.js",
    "dev": "webpack-dev-server --mode development",
    "tsc": "rm -rf dist; tsc -d",
    "tsc:watch": "rm -rf dist; tsc -w -m system -d",
    "lint": "eslint src --ext js,jsx --ignore-path .gitignore --fix",
    "typechain": "typechain --force --outDir src/pools/contracts '.tmp/*.json'"
  },
  "devDependencies": {
    "@0xproject/subproviders": "^0.9.0",
    "@0xproject/typescript-typings": "^0.2.0",
    "@rigoblock/protocol": "^0.1.2",
    "@types/node": "^9.6.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "bignumber.js": "3.0.1",
    "cross-env": "^5.1.1",
    "es6-error": "^4.0.0",
    "es6-promise": "^4.1.1",
    "eslint-plugin-typescript": "^0.11.0",
    "isomorphic-fetch": "^2.2.1",
    "js-sha3": "0.5.5",
    "jsdom": "^11.1.0",
    "lodash": "^4.17.10",
    "ts-loader": "^4.2.0",
    "typechain": "^0.1.5",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0",
    "web3": "0.20.6",
    "web3-provider-engine": "^14.0.4",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  }
}
