{
  "name": "@rigoblock/dapp",
  "version": "0.1.16",
  "private": true,
  "scripts": {
    "start": "serve -s dist -p 8080",
    "ganache": "npx ganache-cli -p 8545 -i 5777 -m \"${npm_package_config_mnemonic}\"",
    "ganache:bootstrap": "ganache-bootstrap",
    "ganache:seed": "ganache-seed",
    "build": "rm -rf dist/* && node scripts/build.js",
    "dev": "node scripts/start.js",
    "test": "npm run test:unit && npm run test:feature",
    "test:unit": "CI=true node scripts/test.js --env=jsdom",
    "test:unit:watch": "node scripts/test.js --env=jsdom",
    "test:unit:debug": "node --inspect-brk scripts/test.js --env=jsdom",
    "test:feature": "rm -rf ./output/* && codeceptjs run --steps",
    "test:feature:debug": "rm -rf ./output/* && codeceptjs run --steps --debug",
    "test:cov": "npm run test:unit -- --coverage",
    "lint": "eslint src --ext js,jsx --ignore-path .gitignore --fix",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o dist-storybook"
  },
  "config": {
    "mnemonic": "lemon scrub wasp bracket town boat property sadness layer taxi butter audit"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-syntax-jsx": "7.0.0",
    "@babel/plugin-transform-react-display-name": "7.0.0",
    "@babel/plugin-transform-react-jsx": "7.0.0",
    "@storybook/addon-info": "^4.0.0-rc.1",
    "@storybook/addon-knobs": "^4.0.0-rc.1",
    "@storybook/addon-options": "^4.0.0-rc.1",
    "@storybook/addon-viewport": "^4.0.0-rc.1",
    "@storybook/addons": "^4.0.0-rc.1",
    "@storybook/react": "^4.0.0-rc.1",
    "@rigoblock/api": "^0.2.11",
    "@rigoblock/ganache-bootstrap": "^0.1.18",
    "autoprefixer": "7.1.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "7.1.4",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "codeceptjs": "^1.4.2",
    "codeceptjs-puppeteer": "^1.3.0",
    "core-js": "3.0.0-beta.3",
    "css-loader": "0.28.11",
    "deep-freeze": "^0.0.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint-loader": "1.9.0",
    "file-loader": "1.1.11",
    "fs-extra": "3.0.1",
    "ganache-cli": "^6.1.8",
    "html-webpack-plugin": "3.0.7",
    "jest": "23.6.0",
    "keymirror": "^0.1.1",
    "localforage": "^1.6.0",
    "lodash-es": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.1",
    "moment-timezone": "^0.5.17",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.7.2",
    "null-loader": "^0.1.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.3",
    "prettier": "^1.11.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "puppeteer": "^1.9.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-md": "^1.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-table": "^6.8.6",
    "reactotron-react-js": "^1.13.0",
    "reactotron-redux": "^1.13.0",
    "redux": "^3.7.2",
    "redux-act": "^1.7.4",
    "redux-form": "^7.3.0",
    "redux-observable": "^0.18.0",
    "redux-persist": "^5.9.1",
    "rxjs": "5",
    "sass-loader": "^6.0.7",
    "serve": "^6.5.5",
    "style-loader": "0.20.3",
    "svg-react-loader": "^0.4.5",
    "sw-precache-webpack-plugin": "0.11.5",
    "updeep": "^1.0.0",
    "url-loader": "0.6.2",
    "web3": "^1.0.0-beta.36",
    "webpack": "4.16.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "3.1.1",
    "webpack-manifest-plugin": "2.0.0-rc.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.js",
      "!src/components/**/index.js",
      "!src/pages/**/index.js",
      "!src/reducers/**/index.js",
      "!src/epics/**/index.js"
    ],
    "coverageDirectory": "../../coverage/dapp",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.svg$": "<rootDir>/config/jest/componentMock.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "clearMocks": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-jsx",
      "@babel/plugin-transform-react-jsx",
      "@babel/plugin-transform-react-display-name",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "^7.1.0",
    "@rigoblock/contracts": "^0.3.6",
    "core-js": "3.0.0-beta.3"
  }
}
